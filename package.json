{
  "name": "swagger-to-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "tsc --project tsconfig.compile.json",
    "postbuild": "node ./scripts/postbuild",
    "eslint": "eslint --fix",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,html,htm,json,css,scss,md}\"",
