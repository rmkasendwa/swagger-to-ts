{
  "name": "swagger-to-ts",
  "version": "0.1.0-alpha.31",
  "description": "Generates typescript api client from open api/swagger api documentation.",
  "keywords": [
    "swagger",
    "swagger-docs",
    "open-api",
    "client",
    "typescript-client",
    "generator",
    "api-client",
    "api-client-generator"
  ],
  "license": "MIT",
  "author": {
    "name": "Ronald M. Kasendwa",
    "email": "kasendwaronald@gmail.com",
    "url": "https://github.com/rmkasendwa"
  },
  "main": "./index.js",
  "bin": {
    "swagger-to-ts": "./run.js"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "tsc --project tsconfig.compile.json",
    "postbuild": "node ./scripts/postbuild",
    "eslint": "eslint --fix",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,html,htm,json,css,scss,md}\"",
    "prepare": "husky install",
    "publish:dev": "npm run build && cd ./dist && npm publish --registry http://localhost:4873 && npm i -g --registry http://localhost:4873 swagger-to-ts@latest",
    "start": "tsnd --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "@infinite-debugger/rmk-js-extensions": "^0.1.0-alpha.11",
    "fs-extra": "^11.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-sort-json": "^1.0.0"
  },
  "devDependencies": {
    "@infinite-debugger/axios-api-adapter": "^0.1.0-alpha.11",
    "@infinite-debugger/rmk-utils": "^0.1.0-alpha.12",
    "@tsed/openspec": "^7.10.0",
    "@types/eslint": "^8.4.10",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "axios": "^1.2.2",
    "chai": "^4.3.7",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^8.0.3",
    "import-sort-style-module": "^6.0.0",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
